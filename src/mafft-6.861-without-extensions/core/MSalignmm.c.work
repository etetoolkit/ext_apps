	FreeFloatVec( initverticalw );
	FreeFloatVec( lastverticalw );

	FreeFloatVec( m );
	FreeIntVec( mp );


	FreeFloatMtx( floatwork );
	FreeIntMtx( intwork );

	FreeShortMtx( shortmtx );

	FreeCharMtx( aseq1bk );
	FreeCharMtx( aseq2bk );

	return( wm );
}

