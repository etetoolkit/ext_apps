	int lgth1, lgth2;
	float *ogcp1 = gapinfo[0];
	float *fgcp1 = gapinfo[1];
	float *ogcp2 = gapinfo[2];
	float *fgcp2 = gapinfo[3];
	static char **aseq1 = NULL;
	static char **aseq2 = NULL;
